#: ../data/mound-data-manager.desktop.in.h:1
msgid "Manage your application data"
msgstr "Gestione los datos de su aplicación"

#: ../Mound/application.py:55
#, python-format
msgid "Error in %(application)s: %(error)s"
msgstr "Error en %(application)s: %(error)s"

#: ../Mound/application.py:104
msgid "Location is not in home directory"
msgstr "La localización no está en el directorio de inicio"

#: ../Mound/application.py:108
msgid "Cannot manage restricted location"
msgstr "No se puede gestionar una localización restringida"

#: ../Mound/application.py:255
msgid "Bad source snapshot"
msgstr "Fuente de instantánea errónea"

#: ../Mound/application.py:286 ../Mound/application.py:298
msgid "Not a Mound snapshot"
msgstr "No es una instantánea de Mound"

#: ../Mound/application.py:301
msgid "Snapshot does not match application"
msgstr "La instantánea no concuerda con la aplicación"

#: ../Mound/application.py:304
msgid "Snapshot already exists"
msgstr "La instantánea ya existe"

#: ../Mound/ui/details.py:102
msgid "This application cannot be managed because of the following problems:"
msgstr ""
"Esta aplicación no puede gestionarse debido a los siguientes problemas:"

#: ../Mound/ui/details.py:106
#, python-format
msgid "%(application)s was loaded from:"
msgstr "%(application)s se ha cargado de:"

#: ../Mound/ui/details.py:109 ../Mound/ui/details.py:111
msgid "Snapshots are stored in:"
msgstr "Las instantáneas se guardan en:"

#: ../Mound/ui/__init__.py:130
msgid "You may want to take a snapshot before continuing."
msgstr "Quizás quiera tomar una instantánea antes de continuar."

#: ../Mound/ui/__init__.py:131
#, python-format
msgid ""
"Are you sure you want to destroy all data, settings, and preferences for "
"%(application)s?"
msgstr ""
"¿Está seguro de que quiere destruir todos los datos, configuraciones y "
"preferencias de %(application)s?"

#: ../Mound/ui/__init__.py:158
#, python-format
msgid "Please close %(application)s before managing it."
msgstr "Por favor, cierre %(application)s antes de gestionarlo."

#. [i18n] %(size)s includes the unit, such as KB or MB.
#: ../Mound/ui/__init__.py:167
#, python-format
msgid "This application is using %(size)s of space."
msgstr "Esta aplicación está usando %(size)s de espacio."

#: ../Mound/ui/__init__.py:169
msgid "This application is not storing any data."
msgstr "Esta aplicación no está almacenando ningún dato."

#: ../Mound/ui/__init__.py:175
msgid "One snapshot is available."
msgstr "Hay una instantánea disponible."

#: ../Mound/ui/__init__.py:177
#, python-format
msgid "%(num)d snapshots are available."
msgstr "Hay %(num)d instantáneas disponibles."

#: ../Mound/ui/__init__.py:181
msgid "A problem occurred. This application cannot be managed."
msgstr "Ha ocurrido un problema. Esta aplicación no puede gestionarse."

#: ../Mound/ui/__init__.py:190
msgid "Select an Application"
msgstr "Seleccione una aplicación"

#: ../Mound/ui/snapshots.py:116
msgid ""
"The snapshot name may only consist of letters, numbers, spaces, underscores, "
"and dashes."
msgstr ""
"El nombre de la instantánea sólo puede consistir en letras, números, "
"espacios, guiones y guiones bajos."

#: ../Mound/ui/snapshots.py:124
msgid "This snapshot already exists."
msgstr "Esta instantánea ya existe."

#: ../Mound/ui/snapshots.py:156
#, python-format
msgid "Are you sure you want to delete the \"%(snap_name)s\" snapshot?"
msgstr ""
"¿Está seguro de que quiere eliminar al instantánea \"%(snap_name)s\"?"

#: ../Mound/ui/snapshots.py:157
#, python-format
msgid "This will not delete any data %(application)s currently uses."
msgstr "Esto no eliminará ningún dato que %(application)s use actualmente."

#: ../Mound/ui/snapshots.py:171
msgid "You may want to take a snapshot before reverting."
msgstr "Quizás quiera tomar una instantánea antes de revertir."

#: ../Mound/ui/snapshots.py:172
#, python-format
msgid "Do you want to revert to the \"%(snap_name)s\" snapshot?"
msgstr "¿Quiere revertir a la instantánea \"%(snap_name)s\"?"

#: ../Mound/ui/snapshots.py:200
#, python-format
msgid ""
"Successfully reverted %(application)s to the \"%(snap_name)s\" snapshot."
msgstr ""
"Se revirtió con éxito %(application)s a la instantánea \"%(snap_name)s\"."

#: ../Mound/ui/snapshots.py:212
msgid "Open Snapshot"
msgstr "Abrir instantánea"

#: ../Mound/ui/snapshots.py:218 ../Mound/ui/snapshots.py:252
msgid "Mound Snapshots"
msgstr "Instantáneas de Mound"

#: ../Mound/ui/snapshots.py:228
msgid "A problem occurred. This snapshot cannot be used."
msgstr "Ocurrió un problema. Esta instantánea no se puede utilizar."

#: ../Mound/ui/snapshots.py:230
msgid "Error:"
msgstr "Error:"

#. ask for a filename
#: ../Mound/ui/snapshots.py:245
msgid "Save Snapshot As"
msgstr "Guardar instantánea como"

